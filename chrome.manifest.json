{
  "name": "mdBook Auto Theme",
  "description": "",
  "version": "0.1.0",
  "manifest_version": 3,
  "content_scripts": [{
    "matches": ["*://*/*"],
    "run_at": "document_end",
    "js": ["page.js"]
  }],
  "permissions": ["storage"],
  "host_permissions": ["*://*/*"],
  "action": {
    "default_popup": "settings.html"
  }
}
