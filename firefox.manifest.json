{
  "name": "mdBook Auto Theme",
  "description": "",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["storage.js"]
  },
  "content_scripts": [
    {
      "matches": ["*"],
      "run_at": "document_idle",
      "js": ["out/pages.bundle.js"]
    }
  ],
  "permissions": [
    "storage",
    "*"
  ]
}
